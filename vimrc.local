nnoremap ,e :e <C-R>=expand('%:p:h') . '/'<CR>
nmap <C-L> :BufExplorer<CR>

" Vim UI {

    if filereadable(expand("~/.vim/bundle/vim-hybrid/colors/hybrid.vim"))
